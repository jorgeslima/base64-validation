{
  "name": "jorgeslima/base64-validation",
  "description": "Laravel validators for base64 encoded files",
  "keywords": [
    "base64",
    "validation",
    "files",
    "encoded",
    "images",
    "download",
    "laravel"
  ],
  "authors": [
    {
      "name": "Jorge Lima",
      "email": "jorge@creativeconcept.com.br"
    }
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1 || ^8.0",
    "laravel/framework": "^5.5|^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Jorgeslima\\Base64Validation\\": "/src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jorgeslima\\Base64Validation\\Providers\\ServiceProvider"
      ]
    }
  }
}
